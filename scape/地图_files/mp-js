function preInitWX_v2(){if(isFromMp!="False"&&noJsSDK!="True"){var n=window.location.href,t=n.indexOf("#");t!=-1&&(n=n.substring(0,t));$.post("/common/JSSDKConfig",{url:n},function(n){n.WxconfigSuccess!=1&&initWX(n.wxConfig,n.WxconfigSuccess)})}}function initWX(n,t){wx.config({debug:!1,appId:n.appId,timestamp:n.timestamp,nonceStr:n.noncestr,signature:n.signature,jsApiList:["onMenuShareTimeline","onMenuShareAppMessage","scanQRCode","chooseImage","uploadImage","onMenuShareQQ","onMenuShareQZone","onMenuShareWeibo","openLocation","startSearchBeacons","stopSearchBeacons","onSearchBeacons","getLocation","hideMenuItems"]});t==0&&(document.addEventListener("WeixinJSBridgeReady",function(){WeixinJSBridge.call("onMenuShareAppMessage");WeixinJSBridge.call("onMenuShareTimeline")}),wx.ready(function(){wx.onMenuShareAppMessage({title:$mcShare.Title,desc:$mcShare.Content,link:$mcShare.Url,imgUrl:$mcShare.Image,type:"link",dataUrl:"",success:function(){},cancel:function(){},fail:function(){}});wx.onMenuShareTimeline({title:$mcShare.Content,link:$mcShare.Url,imgUrl:$mcShare.Image,success:function(){},cancel:function(){},fail:function(){}});wx.onMenuShareQQ({title:$mcShare.Title,desc:$mcShare.Content,link:$mcShare.URL,imgUrl:$mcShare.Image,success:function(){},cancel:function(){}});wx.onMenuShareQZone({title:$mcShare.Title,desc:$mcShare.Content,link:$mcShare.URL,imgUrl:$mcShare.Image,success:function(){},cancel:function(){}});wx.onMenuShareWeibo({title:$mcShare.Title,desc:$mcShare.Content,link:$mcShare.URL,imgUrl:$mcShare.Image,success:function(){},cancel:function(){}})}))}preInitWX_v2()